<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.calvin.calvinfitness">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Create_New_Workout_Activity" />
        <activity android:name=".See_Previous_Workouts_Activity" />
        <activity android:name=".Start_Workout_Activity"></activity>
        <activity android:name=".HomePageAboutActivity"></activity>
        <activity android:name=".StartWorkoutAboutActivity"></activity>
        <activity android:name=".ViewPastWorkoutAboutActivity"></activity>
        <activity android:name=".CreateWorkoutAboutActivity"></activity>
        <activity android:name=".HomePageHelpActivity"></activity>
    </application>

</manifest>